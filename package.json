  "description": "Responsive Swiss railway clock for React",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/designbyadrian/react-mondaine/issues"
  },
  "homepage": "https://github.com/designbyadrian/react-mondaine#readme",
  "dependencies": {
    "@vanilla-extract/css": "^1.2.1",
    "@vanilla-extract/sprinkles": "^1.0.0",
    "date-fns": "^2.23.0",
    "framer-motion": "^4.1.17",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.9",
    "@vanilla-extract/babel-plugin": "^1.0.1",
    "docz": "2.3.1",
    "gatsby-plugin-vanilla-extract": "^1.0.5",
    "get-pkg-repo": "4.1.1",
    "jest": "^27.0.6",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tslint": "6.1.3",
    "tslint-react": "5.0.0",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "/dist",
    "package.json"
  ],
  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "testEnvironment": "jsdom"
  }
